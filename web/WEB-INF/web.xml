<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>servlets.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registrar</servlet-name>
        <servlet-class>servlets.Registrar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginTeacher</servlet-name>
        <servlet-class>servlets.LoginTeacher</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrarTeacher</servlet-name>
        <servlet-class>servlets.RegistrarTeacher</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListarTesis</servlet-name>
        <servlet-class>servlets.ListarTesis</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registrar</servlet-name>
        <url-pattern>/Registrar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginTeacher</servlet-name>
        <url-pattern>/LoginTeacher</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrarTeacher</servlet-name>
        <url-pattern>/RegistrarTeacher</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListarTesis</servlet-name>
        <url-pattern>/ListarTesis</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
